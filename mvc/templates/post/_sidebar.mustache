
<div class="sidebar-item col-xs-12 users-gustan">
	<h3 class="titular"><i class="fa fa-star"></i> {{#transu}}post.le_gusto_a{{/transu}}</h3>
	<hr>
	<section class="users-que-gustan">
	{{#getUsersQueGustanLimitado}}
		{{#lista}}
			{{> post/sidebar/_user_que_gusta}}
		{{/lista}}
		{{#mas}}
			<div class="col-xs-12">
				{{#trans}}y{{/trans}} {{mas}} {{#trans}}mas{{/trans}}.
			</div>
		{{/mas}}
	{{/getUsersQueGustanLimitado}}
	</section>
</div>

{{#haySimilares}}
<div class="sidebar-item col-sm-12 similares">
	<h3 class="titular"><i class="fa fa-group"></i> {{#trans}}sidebar.entradas_similares{{/trans}}</h3>
	<hr>
	<section>
	{{#getSimilares}}
		{{> post/sidebar/_similares}}
	{{/getSimilares}}
	</section>
</div>
{{/haySimilares}}

{{#hayReferencias}}
<div class="sidebar-item col-sm-12 similares">
	<h3 class="titular"><i class="fa fa-code-fork "></i> {{#trans}}sidebar.entradas_relacionadas{{/trans}}</h3>
	<hr>
	<section>
	{{#getReferencias}}
		{{> post/sidebar/_similares}}
	{{/getReferencias}}
	</section>
</div>
{{/hayReferencias}}