<div class="col-xs-12"></div>
<button class="btn btn-default text-center titular col-xs-4 col-sm-3 mostrar-menos" mostrar-menos="{{a_buscar}}">
		<i class="fa fa-minus"></i>
		<span class="visible-xs">{{#transu}}leer_menos_movil{{/transu}}</span>
		<span class="hidden-xs">{{#transu}}leer_menos{{/transu}}</span>
</button>
<button class="btn btn-default text-center titular col-xs-8 col-sm-9 mostrar-mas" mostrar-mas="{{a_buscar}}" url="{{template_url}}/ajax.php" >
