<div class="fila bs-callout bs-callout-success tipo-seguimiento-user">
	{{#getUser}}
		{{> user/actividad/_user}}
	{{/getUser}}
	<span class="texto col-xs-1">
		<i class="fa fa-users fa-3x"></i>
	</span>
	<span class="texto col-xs-7">
		<span class="time col-xs-12 text-left">
			{{> user/actividad/_time }}
		</span>
		{{#getUser.isCurrentUser}}
			{{#trans}}actividad.has_empezado_a_seguir_a{{/trans}} <a href="{{getQue.getPerfilUrl}}">{{getQue.display_name}}</a>.
		{{/getUser.isCurrentUser}}
		{{^getUser.isCurrentUser}}
			{{#getQue.isCurrentUser}}
				<a href="{{getUser.getPerfilUrl}}">{{getUser.display_name}}</a>
				{{#trans}}actividad.ha_empezado_a_seguirte{{/trans}}.
			{{/getQue.isCurrentUser}}
			{{^getQue.isCurrentUser}}
				<a href="{{getUser.getPerfilUrl}}">{{getUser.display_name}}</a>
				{{#trans}}actividad.ha_empezado_a_seguir_a{{/trans}}
				<a href="{{getQue.getPerfilUrl}}">{{getQue.display_name}}</a>.
			{{/getQue.isCurrentUser}}
		{{/getUser.isCurrentUser}}
	</span>
	<span class="avatar2 col-xs-2">
		<a href="{{getQue.getPerfilUrl}}">
			<img src="{{getQue.getAvatar}}">
		</a>
	</span>
</div>